{
  "name": "fastify-lit-element",
  "version": "0.1.0",
  "description": "Fastify plugin for using web components via lit-element",
  "main": "src/plugin",
  "scripts": {
    "dependency:log": "npm list > ./temp/dependencies.log",
    "example": "node example/example",
    "lint": "standard",
    "test:unit": "tap -J test/*.test.js test/*/*.test.js",
    "test:unit:debug": "tap -T --strict --node-arg=--inspect-brk test/*.test.js test/*/*.test.js",
    "test": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "fastify-plugin": "^1.5.0",
    "lit-element": "^2.0.1"
  },
  "devDependencies": {
    "fastify": "^1.14.1",
    "standard": "^12.0.1",
    "tap": "^12.5.3"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=8.15.0"
  },
  "homepage": "https://github.com/smartiniOnGitHub/fastify-lit-element#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartiniOnGitHub/fastify-lit-element.git"
  },
  "bugs": {
    "url": "https://github.com/smartiniOnGitHub/fastify-lit-element/issues"
  },
  "keywords": [
    "fastify",
    "plugin",
    "web component",
    "lit-html"
  ],
  "author": "Sandro Martini <sandro.martini@gmail.com>",
  "license": "MIT"
}
